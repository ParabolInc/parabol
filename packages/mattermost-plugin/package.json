    "@types/franc": "^5.0.3",
    "@types/node": "^16.11.62",
    "@types/react": "16.9.11",
    "babel-plugin-inline-import": "^3.0.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "jest-junit": "^16.0.0",
    "openapi-fetch": "^0.10.0",
    "react": "^17.0.2",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^1.0.0-pre.44"
