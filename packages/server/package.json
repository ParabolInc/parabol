  "scripts": {
    "start": "cross-env NODE_ENV=production pm2-runtime start pm2.config.js",
    "start:tunnel": "export $(grep ^ULTRAHOOK_API_KEY .env | tr -d '\"') && ultrahook -k $ULTRAHOOK_API_KEY dev 3000",
    "start:memwatch": "NODE_ENV=production MEMWATCH=true node --expose-gc --inspect --trace-warnings server.babel.js",
    "quickstart": "yarn db:migrate && yarn bs",
    "dev": "node webpack/buildDll.js && node server.babel.js",
    "bs": "yarn build && yarn start",
    "build": "yarn build:relay && yarn build:client",
    "build:client": "webpack --config webpack/webpack.prod.config.js",
    "build:deploy": "export WEBPACK_DEPLOY=true && yarn build",
    "build:relay": "yarn update-schema && yarn build:relay-unsafe",
    "build:relay-unsafe": "relay-compiler",
    "build:stats": "export WEBPACK_STATS=true && yarn build:client",
    "db:migrate": "yarn db:migrate-up-all",
    "db:migrate-testing": "export NODE_ENV=test && yarn db:migrate && node __tests__/setup/softenDurability.babel.js",
    "db:migrate-up": "node database/migrate.babel.js up",
    "db:migrate-up-all": "node database/migrate.babel.js up --all",
    "db:migrate-down": "node database/migrate.babel.js down",
    "db:migrate-down-all": "node database/migrate.babel.js down --all",
    "db:migrate-create": "node database/migrate-create.babel.js",
    "debug": "node --nolazy --inspect=9229 server.babel.js",
    "debug:prod": "NODE_ENV=production node --nolazy --inspect=9229 server.babel.js",
    "debug:stripe": "node billing/debug.babel.js",
    "postdeploy": "node utils/postDeploy.babel.js",
    "precommit": "lint-staged",
    "lint": "eslint --fix ./**/*.ts",
    "test": "jest --bail --coverage --ci --runInBand && codecov",
    "test:dev": "jest --runInBand --watch",
    "relay": "yarn build:relay --watch --watchman true --quiet false",
    "update-schema": "node utils/updateSchema.babel.js && yarn tsgen",
    "postversion": "git push --follow-tags",
    "tsgen": "gql2ts ../../build/schema.json -e $(pwd)/../client/types/config.js -o ../client/types/graphql.ts && eslint --fix '../client/types/graphql.ts'",
    "tsgen:gh": "gql2ts utils/githubSchema.json -e $(pwd)/../client/types/configGH.js -o packages/client/types/githubGraphql.d.ts && eslint --fix 'packages/client/types/githubGraphql.d.ts'",
    "tsgen:ghQuery": "graphql-typescript-definitions --schema-types-path . && eslint --fix 'packages/client/utils/githubQueries/**/*.d.ts' ",
    "typecheck": "tsc --noEmit"
  },
