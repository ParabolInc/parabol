{
  "name": "parabol-action",
  "description": "An open-source app for building smarter, more agile teams.",
  "author": "Parabol Inc. <love@parabol.com> (http://github.com/ParabolInc)",
  "license": "AGPL-3.0",
  "version": "2.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParabolInc/action"
  },
  "homepage": "https://github.com/ParabolInc/action",
  "bugs": {
    "url": "https://github.com/ParabolInc/action/issues"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/server/server.babel.js",
    "start:tunnel": "export $(grep ^ULTRAHOOK_API_KEY .env | tr -d '\"') && ultrahook -k $ULTRAHOOK_API_KEY dev 3000",
    "start:memwatch": "NODE_ENV=production MEMWATCH=true node --expose-gc --inspect --trace-warnings ./src/server/server.babel.js",
    "quickstart": "npm run db:migrate && npm run bs",
    "dev": "if [ ! -f \"dll/vendors.json\" ] ; then npm run build:dll ; fi ; node ./src/server/server.babel.js",
    "bs": "npm run build && npm start",
    "build": "npm run build:relay && npm run build:client",
    "build:client": "webpack --config ./webpack/webpack.prod.config.js",
    "build:deploy": "export WEBPACK_DEPLOY=true && npm run build",
    "build:dll": "webpack --config ./webpack/webpack.config.dll.js",
    "build:relay": "npm run update-schema && relay-compiler --src ./src --schema build/schema.json --client-schema src/client/clientSchema.graphql",
    "build:stats": "export WEBPACK_STATS=true && npm run build",
    "db:migrate": "npm run db:migrate-up-all",
    "db:migrate-testing": "export NODE_ENV=test && npm run db:migrate && node ./src/server/__tests__/setup/softenDurability.babel.js",
    "db:migrate-up": "node ./src/server/database/migrate.babel.js up",
    "db:migrate-up-all": "node ./src/server/database/migrate.babel.js up --all",
    "db:migrate-down": "node ./src/server/database/migrate.babel.js down",
    "db:migrate-down-all": "node ./src/server/database/migrate.babel.js down --all",
    "db:migrate-create": "cd ./src/server/database && migrate-rethinkdb create",
    "debug:stripe": "node ./src/server/billing/debug.babel.js",
    "postcommit": "git update-index --again",
    "postdeploy": "node ./src/server/utils/postDeploy.babel.js",
    "precommit": "lint-staged",
    "flow": "flow",
    "prettier": "prettier --write --loglevel warn 'src/**/*.js' 'webpack/*.js' '.storybook/*.js' 'stories/**/*.js'",
    "standard": "standard --fix 'src/**/*.js' 'webpack/*.js' '.storybook/*.js' 'stories/**/*.js' | snazzy",
    "test": "jest --bail --coverage --ci --runInBand && codecov",
    "test:dev": "jest --runInBand --watch",
    "test:e2e": "node ./e2e/scripts/runTests.babel.js",
    "test:e2e-deps": "node ./e2e/scripts/ensureDrivers.babel.js",
    "test:intranetToken": "node ./src/server/utils/getIntranetToken.babel.js",
    "lint": "yarn standard && yarn flow",
    "relay": "npm run update-schema && relay-compiler --src ./src --schema build/schema.json --client-schema src/client/clientSchema.graphql --watch",
    "update-schema": "node ./src/server/utils/updateSchema.babel.js && yarn flowgen",
    "postversion": "git push --follow-tags",
    "flowgen": "gql2flow -e -o ./src/universal/types/schema.flow.js ./build/schema.json && prettier --write 'src/universal/types/schema.flow.js'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
