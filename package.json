  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ]
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testRegex": "/__tests__/.*.test\\.jsx?$",
    "testResultsProcessor": "./src/server/__tests__/teardown",
    "setupTestFrameworkScriptFile": "./src/universal/__tests__/enzyme"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "__ACTION__",
      "__CLIENT__",
      "__PRODUCTION__",
      "graphql",
      "$ReadOnlyArray",
      "$Shape",
      "SyntheticKeyboardEvent",
      "System"
    ],
    "ignore": [
      "schema.flow.js"
    ]
  }
